	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch via NPM",
			"request": "launch",
			"runtimeArgs": [
				"start"
			],
			"runtimeExecutable": "npm",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node",
			"env": {
				"DB_URL": "mongodb://localhost:27017/portfolio-service"
			},
			"console": "integratedTerminal"
		}
	]
}